[Service]
ExecStartPre=-/usr/bin/docker kill redis-abassador-proxy%i
ExecStartPre=-/usr/bin/docker rm %redis-abassador-proxy%i
ExecStart=/usr/bin/docker run -rm -name %redis-abassador-proxy%i polvi/simple-amb 172.17.42.1:4001
ExecStop=/usr/bin/docker stop -t 3 %redis-abassador-proxy%i

[X-Fleet]
X-Conflicts=redis-server-master@%i.service
